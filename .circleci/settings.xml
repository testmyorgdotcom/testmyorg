<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <servers>
    <server>
      <id>ossrh</id>
      <username>env.OSSRH_USERNAME</username>
      <password>env.OSSRH_PASSWORD</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>release</id>
      <properties>
        <gpg.executable>gpg2</gpg.executable>
        <gpg.passphrase>env.GPG_PASSPTHRASE</gpg.passphrase>
      </properties>
    </profile>
  </profiles>
</settings>
